{
    "key": "agile-smells",
    "name": "Agile Smells",
    "description": "Something doesn't smell right about your agile practices? These charts can point you in the right direction",
    "vendor": {
        "name": "Brand Name Software",
        "url": ""
    },
    "baseUrl": "{{localBaseUrl}}",
    "enableLicensing": true,
    "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}/atlassian-connect.json"
    },
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [
        "read"
    ],
    "modules": {
        "jiraReports": [
        {
          "key": "issues-added-to-sprints-report",
          "name": {
      		    "value": "Issues added to sprints"
        	},
        	"description": {
        		"value": "See how much scope is changing during an iteration. This will help understand if the team's goals are constantly in flux."
        	},
          "url": "/changed-issues-report?project.key={project.key}&project.id={project.id}&board.id={board.id}",
          "reportCategory": "agile",
          "configurable": true,
          "thumbnailUrl": "/thumbnail.png"
        }
      ]
    }
}
