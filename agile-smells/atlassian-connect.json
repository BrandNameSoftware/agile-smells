{
    "key": "agile-smells",
    "name": "Agile Smells",
    "description": "Something doesn't smell right about your agile practices? These charts can point you in the right direction",
    "vendor": {
        "name": "Brand Name Software",
        "url": "https://www.atlassian.com/angrynerds"
    },
    "baseUrl": "{{localBaseUrl}}",
    "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}/atlassian-connect.json"
    },
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [
        "read"
    ],
    "modules": {
        "generalPages": [
            {
                "key": "hello-world-page-jira",
                "location": "system.top.navigation.bar",
                "name": {
                    "value": "Hello World"
                },
                "url": "/graph",
                "conditions": [{
                    "condition": "user_is_logged_in"
                }]
            }
        ]
    }
}
